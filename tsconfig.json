  "compilerOptions": {
    "module": "NodeNext",
    "preserveValueImports": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noEmit": true,
    "isolatedModules": true,
    "moduleResolution": "NodeNext",
    "allowImportingTsExtensions": true
  }
